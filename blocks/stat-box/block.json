{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "wpamesh/stat-box",
    "version": "1.0.0",
    "title": "Stat Box",
    "category": "widgets",
    "icon": "dashboard",
    "description": "Displays a single network statistic from the Meshview API.",
    "keywords": ["stat", "statistic", "nodes", "network", "mesh", "counter"],
    "textdomain": "wpamesh-theme",
    "attributes": {
        "stat": {
            "type": "string",
            "default": "total_nodes"
        }
    },
    "supports": {
        "html": false,
        "align": false,
        "multiple": true
    },
    "render": "file:./render.php",
    "editorScript": "file:./editor.js"
}
